# Hyphae POS Environment Variables
# Copy this file to .env and fill in your values

# ------------------------------
# 1. API Keys & Integration
# ------------------------------
# Google Gemini API Key for AI features (Menu generation, Analytics)
GEMINI_API_KEY=your_gemini_api_key_here

# ------------------------------
# 2. Application Flags
# ------------------------------
# Enable/Disable Debug Mode (0=False, 1=True)
VITE_DEBUG_MODE=0

# Backend URL (if applicable)
VITE_API_BASE_URL=http://localhost:4000
